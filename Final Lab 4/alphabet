.MODEL SMALL
.STACK 100H
.DATA 
MSG1 DB "?$"
MSG2 DB 0DH,0AH,"ENTER THE 2 LETTER :$"
MSG3 DB 0DH,0AH,"CONVERTING THE ALPHABETIC ORDER :$"
.CODE
MAIN PROC
    MOV AX,@DATA
    MOV DS,AX
    
    LEA DX,MSG1
    MOV AH,9
    INT 21H
    
    LEA DX,MSG2
    MOV AH,9
    INT 21H
    
    MOV AH,1
    INT 21H
    MOV BL,AL
    
    MOV AH,1
    INT 21H
    MOV BH,AL
    
    CMP BL,BH
    JLE  NO_CHANGE
    XCHG BL,BH
    
    NO_CHANGE:
    
    LEA DX,MSG3
    MOV AH,9
    INT 21H
    
    MOV AH,2
    MOV DL,BL
    INT 21H
    
    MOV AH,2
    MOV DL,BH
    INT 21H
    
    MOV AH,4CH
    INT 21H
    
    MAIN ENDP
END MAIN
